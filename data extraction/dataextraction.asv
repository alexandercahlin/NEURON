fastact = importfile('C:\Users\Alexander\Desktop\Lundbeck data.xlsx', 'Activation','B5:AZ16');

nrIgnCells=2;
TotalCells=13;
k=zeros(TotalCells-nrIgnCells,1);
v=zeros(TotalCells-nrIgnCells,1);
r2=zeros(TotalCells-nrIgnCells,1);


for i=0:TotalCells-1 && i~=8 && i~=10
        x=fastact(:,1+4*i);
        y=fastact(:,2+4*i);
        [sigmoid, gof] = sigmoidfit(x, y);
        r2(i+1)=gof.rsquare;
        k(i+1)=sigmoid.k;
        v(i+1)=sigmoid.v;
end

kmean=mean(k);
vmean=mean(v);
