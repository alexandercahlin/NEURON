//@code
objref gm
gm = new GUIMath()
{printf("default rounding mode %d\n", gm.feround())}

proc test_round() {local i, old, x  localobj gm
        gm = new GUIMath()
        old = gm.feround($1)
        x = 0
        for i=1, 1000000 x += 0.1
        printf("rounding mode %d x=%25.17lf\n", $1, x)
        gm.feround(old)
}

for i=1, 4 test_round(i)
